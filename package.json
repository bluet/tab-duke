{
  "name": "tab-duke",
  "description": "Tab Duke is a heavily rewritten, updated fork of Tab Count and is inspired by Tabman Tabs Manager.  It is a Chrome extension that displays the count and list of open tabs from the current or all windows (configurable). It also allows you to select and/or close them.",
  "version": "0.1.1",
  "devDependencies": {
    "@babel/eslint-parser": "^7.23.3",
    "eslint": "^8.55.0",
    "prettier-eslint": "^16.1.2",
    "tailwindcss": "^3.4.19"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-css": "npx tailwindcss -i ./src/tailwind.css -o ./dist/tailwind.min.css --minify",
    "watch-css": "npx tailwindcss -i ./src/tailwind.css -o ./dist/tailwind.min.css --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/bluet/tab-count.git"
  },
  "keywords": [
    "tab",
    "management",
    "chrome"
  ],
  "author": "BlueT - Matthew Lien <bluet@bluet.org>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bluet/tab-count/issues"
  },
  "homepage": "https://github.com/bluet/tab-count#readme",
  "dependencies": {
    "baseline-browser-mapping": "^2.9.7",
    "caniuse-lite": "^1.0.30001760"
  }
}
